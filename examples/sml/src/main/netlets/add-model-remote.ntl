#!/usr/bin/env nsh

@Codebase(group='org.sorcer', module='sml', version='${sorcer.version}', classifier='dl')
import sorcer.arithmetic.provider.Adder

// model three entries
model(inEnt("arg/x1", 10.00), inEnt("arg/x2", 90.00),
        ent(sig("add", Adder.class, result("result/y", inPaths("arg/x1", "arg/x2")))),
        response("add", "arg/x1", "arg/x2"));
